mixin footer

  +b.FOOTER.l-footer
    +b-contact-form

    +e.socail
      +b.m-footer-logo-list
        +e.list
          +e.item
            a(href="#")
              i.i-icon-facebook
          +e.item
            a(href="#")
              i.i-icon-instagram
          +e.item
            a(href="#")
              i.i-icon-twitter
          +e.item
            a(href="#")
              i.i-youtube

    +e.privacy
      +b.m-privacy-list
        +e.list
          +e.item
            a(hre="#") CONTACT US
          +e.item
            a(hre="#") TERMS &amp; CONDITIONS
          +e.item
            a(hre="#") PRIVACY POLICY

    +e.menu
      +b-dropdown-menu('INTERNATIONAL WEBSITES', 'CANADA (French)', 'GERMANY', 'HONG KONG (English)', 'HONG KONG (Traditional Character)', 'HONG KONG (Simplified Character)')
      +b-dropdown-menu('OUR WINE BRANDS', 'CANADA (French)', 'GERMANY', 'HONG KONG (English)', 'HONG KONG (Traditional Character)', 'HONG KONG (Simplified Character)')
      +b-dropdown-menu('CORPORATE', 'couple', 'test', 'items')
    +e.logos
      +b.m-footer-logos
        +e.UL.list
          +e.item.--first
            img(src="img/MCWILLIAMS-WORDMARK_MONO.png" alt="")
          +e.item.--second
            img(src="img/AFFW_horiz_lg_BW.png" alt="")
          +e.item.--third
            img(src="img/Drinkwise_logo.png" alt="")



    +e.copyright
      +b.m-copyright
        p New South Wales Liquor Licence: LIQP700383194. Liquor Act 2007. It is against the law to sell or supply alcohol to, or to obtain alcohol on behalf of, a person under the age of 18 years.


